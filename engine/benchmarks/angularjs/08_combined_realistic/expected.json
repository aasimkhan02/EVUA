{
  "supported_constructs": [
    "controller",
    "service",
    "$http",
    "$q",
    "ui-router",
    "ng-repeat",
    "ng-model",
    "custom_directive",
    "$compile",
    "transclusion",
    "$watch_deep"
  ],
  "auto_modernized": [
    "UserCtrl -> Angular Component",
    "UserService -> Injectable + HttpClient",
    "ui-view -> router-outlet",
    "ui-sref -> routerLink",
    "ng-repeat -> *ngFor",
    "ng-model -> [(ngModel)]"
  ],
  "manual_required": [
    "directive:myPanel",
    "$compile",
    "transclusion",
    "$watch(user, deep=true)"
  ],
  "blocked": [
    "$compile",
    "transclusion",
    "$watch_deep"
  ],
  "notes": [
    "This repo intentionally mixes SAFE, RISKY, and MANUAL constructs",
    "Overall validation should fail due to manual blockers"
  ]
}
